{
    "name": "php-flasher/flasher-noty-laravel",
    "description": "PHPFlasher Laravel adapter for Noty",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "yoeunes",
        "notify",
        "php",
        "laravel",
        "symfony",
        "Lumen",
        "notifications",
        "messages",
        "alerts",
        "pnotify",
        "noty ",
        "bundle",
        "flex"
    ],
    "authors": [
        {
            "name": "Younes KHOUBZA",
            "email": "younes.khoubza@gmail.com",
            "homepage": "https://www.linkedin.com/in/younes-khoubza",
            "role": "Developer"
        }
    ],
    "homepage": "https://php-flasher.io",
    "require": {
        "php": ">=5.3",
        "php-flasher/flasher-laravel": "^1.10.6",
        "php-flasher/flasher-noty": "^1.10.6"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Flasher\\Noty\\Laravel\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Noty": "Flasher\\Laravel\\Facade\\Flasher"
            },
            "providers": [
                "Flasher\\Noty\\Laravel\\FlasherNotyServiceProvider"
            ]
        }
    }
}
