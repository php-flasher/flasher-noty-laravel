{
    "name": "php-flasher/flasher-noty-laravel",
    "type": "library",
    "description": "PHP Flasher Laravel adapter for Noty",
    "keywords": ["yoeunes", "notify", "php", "laravel", "symfony", "Lumen", "notifications", "messages", "alerts", "pnotify", "noty ", "bundle", "flex"],
    "homepage": "https://php-flasher.io",
    "authors": [
        {
            "name": "Younes KHOUBZA",
            "email": "younes.khoubza@gmail.com",
            "homepage": "https://www.linkedin.com/in/younes-khoubza",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3",
        "php-flasher/flasher-laravel": "^1.4",
        "php-flasher/flasher-noty": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Flasher\\Noty\\Laravel\\": ""
        },
        "files": [
            "noty.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flasher\\Noty\\Laravel\\FlasherNotyServiceProvider"
            ],
            "aliases": {
                "Noty": "Flasher\\Laravel\\Facade\\Flasher"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
